class Solution {
public:
    int search(vector<int>& v, int key) 
    {
        int st=0,en=v.size()-1;
        while(st<=en)
        {
            int md=st+(en-st)/2;
            if(v[md]==key)
                return md;
            if(v[md]>v[en])
            {
                if(key>v[md])
                    st=md+1;
                else{
                    if(v[st]<=key && key<v[md])
                        en=md-1;
                    else
                        st=md+1;
                }
            }
            else{
                if(v[md]<key && key<=v[en])
                    st=md+1;
                else
                    en=md-1;
            }
        }
        return -1;
    }
};
